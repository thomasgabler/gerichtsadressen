         Formular zurücksetzen </a>
</div>
</form>
	
</div>

<script language="javascript" type="text/javascript">

$(document).ready(function() {
    $("#erweiterteSuche_Submit").show();

	
		runAutoComplete(document.getElementById('esName'), 'https://dienste.kvb.de/arztsuche/app/suggestNameFeld.htm');
	

	if (document.getElementById("bezirksstellen").value != "" &&
			document.getElementById("landkreise").value == "") {
			document.getElementById("landkreise").disabled = true;
		}

	if (document.getElementById("landkreise").value != "" &&
			document.getElementById("bezirksstellen").value == "") {
			document.getElementById("bezirksstellen").disabled = true;
		}
			
 $('a#moreInfo1').click(function() {
   $('#moreInfoDiv1').slideToggle(700);
     if ($('a#moreInfo1').text().indexOf('[+] ausklappen')>-1) {
       $('a#moreInfo1').text('[-] zuklappen');
       createEinstellungenCookie(1, true); //bitwise 1 an der ersten Stelle
     }
     else {
       $('a#moreInfo1').text('[+] ausklappen');
       createEinstellungenCookie(1, false); 
     }      
     $('a#moreInfo1').blur();

   return false;
 });



 $('a#moreInfo2').click(function() {
   $('#moreInfoDiv2').slideToggle(700);
   if ($('a#moreInfo2').text().indexOf('[+] ausklappen')>-1) {
       $('a#moreInfo2').text('[-] zuklappen');
       createEinstellungenCookie(2, true);	//bitwise 1 an der zweiten Stelle
   } else {
       $('a#moreInfo2').text('[+] ausklappen');
       createEinstellungenCookie(2, false);
     }
   $('a#moreInfo2').blur();
   return false;
 });

  $('a#moreInfo1').css("display", "");
  $('a#moreInfo2').css("display", "");

  var cookieValueString = readEinstellungenCookie();
  var cookieValue = parseInt(cookieValueString);

	
		if (!(cookieValue & 1)) {
      		$('a#moreInfo1').text('[+] ausklappen');
      		$('#moreInfoDiv1').hide();
  		}
	

		  
		if (!(cookieValue & 2)) {
	  		$('a#moreInfo2').text('[+] ausklappen');
	  		$('#moreInfoDiv2').hide();
  		}
  	

});

var browser=navigator.appName;
var b_version=navigator.appVersion;
var version=parseFloat(b_version);
if (browser.indexOf('Microsoft Internet') && version > 3) {
	document.getElementById("ER_SUCHE").style.cssFloat="left";
}

</script>




