        <noscript>
            <input type="image" src="/arztsuche/project/img/suche_starten.png" style="margin-left: 400px; margin-bottom:30px; margin-top:30px;" title="Suche starten" name="cargoSearch" />
        </noscript>
	    <input type="hidden" id="geocodeResult" name="geocodeResult" value="" />
        <!-- Wird für die Test benötigt, um die GEO-Daten-Ertmittlung Serverseitig auszuführen. -->
        <input type="hidden" id="geoTest" name="geoTest" value="" />
