<script language="javascript" type="text/javascript">  

	var isAutoComplete=true;
	var cookieNameEinstellungen = "arztsucheEinstellungen";

	function createEinstellungenCookie(bitwiseValue, extended) {
		var currentCookieValue = readCookie(cookieNameEinstellungen);
		var cookieValue = 0;

     	if (currentCookieValue != null && currentCookieValue.length > 0) {
			cookieValue = parseInt(currentCookieValue);
		}

		if (extended == true) {
			cookieValue += bitwiseValue;
		}
		else {
			cookieValue -= bitwiseValue;
		}

		if (cookieValue < 0) cookieValue = 0;

		createCookie(cookieNameEinstellungen, cookieValue);
	}

	function createCookie(key, value) {
		var expiresDate = new Date();
		expiresDate.setTime(expiresDate.getTime() + 2592000000); //ein Monat
		var expires = "; expires=" + expiresDate.toGMTString(); 

    	document.cookie = key + "=" + value + expires + "; path=" + "/arztsuche"; 
	}

	function readEinstellungenCookie(){
		var value = readCookie(cookieNameEinstellungen);
		return value;
	}

	function readCookie(key) {
		var cookieName = key + "=";
		var cookieParts = document.cookie.split(';');
		for (var i=0; i<cookieParts.length; i++) {
			var cookieString = cookieParts[i];

			//wir löschen die ' ' am Anfang des Strings
			while (cookieString.charAt(0) == ' ') cookieString = cookieString.substring(1, cookieString.length);
			if (cookieString.indexOf(key) == 0) return unescape(cookieString.substring(cookieName.length, cookieString.length));
		}
		return null;
	}

	function switchOffGenehmigungen(value) {
		switchOffAuswahlList(value, "genehmigungen");
	}

	function switchOffLandkreise(value) {
		switchOffAuswahlList(value, "landkreise");
	}

	function switchOffBezirksstellen(value) {
		switchOffAuswahlList(value, "bezirksstellen");
	}

	function switchOffAuswahlList(value, elementId) {
		var disabled = false;
		if (value != ""){	//keine Auswahl
			disabled = true;
			document.getElementById(elementId).value = "";
		}
		document.getElementById(elementId).disabled = disabled;
	}

</script> 
	
	
	<div style="clear:both;  background-color:#FFFFFF; border-top:1px solid #C9CFDE; width:100%; ">
        <!-- Dieser Button wird für die Clientseitige GEO-Daten-Ermittlung benötigt und darf nicht entfernt werden -->
